import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int path=(a/10)+(a%10);
        System.out.println("Bird said:"+a);
        System.out.println("Alice must go in path-"+path);
        
        
    }
}
